   <div class="container-fluid ">
  <div class="row">

    <div class="col-sm-4">
  <div class="row">
    <div class="col-sm-12">
      <h1 class="animated rollIn" id="top1">SignUp and Create Profile</h1>
    </div>
  </div>

  <div class="row">
    <div class="col-sm-4">
      <img src="/stylesheets/shiva.jpg" id="img1"class="animated bounceIn">
    </div>
    <div class="col-sm-8">
      <div  id="p1" class="animated bounceInRight para">
        <h4>Create Username that must contain <b>6 character</b></h4>
        </div>
    </div>
  </div>

  <div class="row">
    <div class="col-sm-4">
      <img src="/stylesheets/shivani.jpg" id="img2" class="animated bounceIn">
    </div>
    <div class="col-sm-8">
      <div  id="p2" class="animated bounceInRight "><h4><b>Create Password</b></h4><br>
        <ul >
          <li><h5>One Upper case alphabet</h5></li>
          <li><h5>One Number</h5></li>
          <li><h5>One special symbol</h5></li>
        </ul>
      </div>
    </div>
  </div>

  <div class="row">
    <div class="col-sm-4">
      <img src="/stylesheets/Profile.jpg" style="image-radius:50px;" id="img3" class="animated bounceIn">
    </div>
    <div class="col-sm-8">
      <div id="p3" class="animated bounceInRight "><h4><b>Create Profile</b></h4><br>
        <ul>
        <li><h5>Fill Pesonnel Details</h5></li>
          <li><h5>Fill Educational Details</h5></li>
          <li><h5>Fill Other Details</h5></li>
      </div>
    </div>
  </div>

  <div class="row">
    <div id="img4" class="col-sm-4 animated bounceIn">
      <img src="/stylesheets/submit.png">
    </div>
    <div class="col-sm-6">
      <h4 id="p4"  class="animated bounceIn para">Click Submit Button</h4>
    </div>
  </div>
</div>







<div class="col-sm-4">
 <div class="row">
  <div class="col-sm-12 animated rollIn" id="top2">
    <h1 >Login</h1>
    </div>
 </div>
 <div class="row">
  <div class="col-sm-4 animated bounceIn" id="img5">
    <img src="/stylesheets/u2.png">
  </div>
  <div class="col-sm-8 animated bounceInRight para" id="p5">
    <h4 class="para">Please Enter <b>Valid</b> Username created while SignUp</h4>
  </div>
 </div>
 <div class="row">
  <div class="col-sm-4 animated bounceIn" id="img6">
    <img src="/stylesheets/shivani.jpg">
  </div>
  <div class="col-sm-8 animated bounceInRight para" id="p6">
    <h4 class="para">Please Enter <b>Valid</b> Password created while SignUp</h4>
  </div>
 </div>
 <div class="row">
  <div class="col-sm-4 animated bounceIn" id="img7">
    <img src="/stylesheets/log.png">
  </div>
  <div class="col-sm-8 animated bounceInRight para" id="p7">
    <h4 class="para">Click On Login Button</h4>
  </div>
 </div>
</div>

<div class="col-sm-4">
  <div class="row animated rollIn" id="top3">
